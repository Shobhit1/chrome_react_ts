{
  "name": "Getting Started Extension",
  "description": "Getting hands dirty",
  "version": "0.0",
  "manifest_version": 2,
  "page_action": {
    "default_popup": "./index.html"
  },
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens Hello Chrome extension"
    }
  }
}
